		pinctrl-names = "cam_default", "cam_suspend";
		pinctrl-0 = <0x12f 0x130>;
		pinctrl-1 = <0x131 0x132>;
		gpios = <0xa7 0x1a 0x0 0xa7 0x24 0x0 0xa7 0x23 0x0>;
		qcom,gpio-reset = <0x1>;
		qcom,gpio-standby = <0x2>;
